msgstr ""

#: /data/convos/5336a176336331280100328f.convo:ConversationLink:responsetext
msgid "Slag-Off."
msgstr ""

#: /data/convos/5336a176336331280100328f.convo:ConversationLink:responsetext
msgid "I'm sorry, but I'd rather take my chances."
msgstr ""

#: /data/convos/5336a176336331280100328f.convo:ConversationNode:text
msgid "Wait. What?"
msgstr ""

#: /data/convos/5336a176336331280100328f.convo:ConversationNode:text
